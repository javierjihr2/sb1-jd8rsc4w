{
  "name": "squadgo-battle",
  "version": "0.1.0",
  "private": true,
  "description": "SquadGO - ¡Encuentra amigos ya!",
  "scripts": {
    "dev": "next dev -p 9002 -H 0.0.0.0",
    "dev:local": "next dev -p 3000",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build && node scripts/fix-paths.js && npx cap sync",
    "build:web": "next build",
    "deploy": "npm run build:web && firebase deploy --only hosting",
    "deploy:custom-domain": "npm run build:web && firebase deploy --only hosting && echo 'Deploy completado. Verifica tu dominio personalizado.'",
    "setup-domain": "node scripts/setup-custom-domain.js",
    "verify-domain": "echo 'Verificando configuración DNS...' && nslookup",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:functions": "cd functions && npm test",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug"
  },
